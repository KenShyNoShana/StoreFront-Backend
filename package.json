{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "type": "commonjs",
  "main": "server.ts",
  "scripts": {
    "start": "cross-env ENV=dist nodemon ./src/app.ts",
    "startL": "ENV=dist nodemon ./src/app.ts",
    "watch": "cross-env ENV=dist tsc-watch --esModuleInterop src/app.ts --outDir ./dist --onSuccess \"node ./dist/app.js\"",
    "watchL": "ENV=dist tsc-watch --esModuleInterop src/app.ts --outDir ./dist --onSuccess \"node ./dist/app.js\"",
    "test": "cross-env ENV=test npm run testSetup",
    "testL": "ENV=test npm run testSetup",
    "testSetup": "db-migrate --env test down -c5 && db-migrate --env test up && npm run jestSetup",
    "jestSetup": "jest -- orders.test.ts && jest -- users.test.ts && jest -- products.test.ts",
    "db-up": "cross-env ENV=dev db-migrate up --config ./database.json",
    "db-upL": "ENV=dev db-migrate up --config ./database.json",
    "db-down": "cross-env ENV=dev db-migrate down -c5 --config ./database.json",
    "db-downL": "ENV=dev db-migrate down -c5 --config ./database.json",
    "tsc": "tsc"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/jest": "^28.1.5",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.6",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.12",
    "cross-env": "^7.0.3",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.6",
    "ts-node": "^10.9.1",
    "tsc-watch": "^4.2.9"
  }
}
